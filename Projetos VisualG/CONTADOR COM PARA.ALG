Algoritmo "Contador com para"

Var
   at, d, C, qq: inteiro

Inicio
      Escreval("De :")
      leia(d)
      Escreval("Até quanto? ")
      leia(at)
      Escreval("De quanto em quanto?")
      leia(qq)
      Para C <- d ate at  passo qq faça
           Escreval(C ,"...")
      Fimpara


Fimalgoritmo